<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">

        <Window name="PHGroupWindow">
            <Size>
                <AbsPoint x="200" y="150" />
            </Size>
            <Windows>
                <Label name="$parent1" inherits="EA_Label_DefaultText" textalign="leftcenter">
                    <Size>
                        <AbsPoint x="200" y="26" />
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Label>
                <Label name="$parent2" inherits="EA_Label_DefaultText" textalign="leftcenter">
                    <Size>
                        <AbsPoint x="200" y="26" />
                    </Size>
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parent1">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Label>
                <Label name="$parent3" inherits="EA_Label_DefaultText" textalign="leftcenter">
                    <Size>
                        <AbsPoint x="200" y="26" />
                    </Size>
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parent2">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Label>
                <Label name="$parent4" inherits="EA_Label_DefaultText" textalign="leftcenter">
                    <Size>
                        <AbsPoint x="200" y="26" />
                    </Size>
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parent3">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Label>
                <Label name="$parent5" inherits="EA_Label_DefaultText" textalign="leftcenter">
                    <Size>
                        <AbsPoint x="200" y="26" />
                    </Size>
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parent4">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Label>
                <Label name="$parent6" inherits="EA_Label_DefaultText" textalign="leftcenter">
                    <Size>
                        <AbsPoint x="200" y="26" />
                    </Size>
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parent5">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Label>
            </Windows>
        </Window>

        <Window name="PHWarbandWindow">
            <Size>
                <AbsPoint x="800" y="150" />
            </Size>
            <Windows>
                <Window name="$parentGroup1" inherits="PHGroupWindow">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent" >
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentGroup2" inherits="PHGroupWindow">
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentGroup1" >
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentGroup3" inherits="PHGroupWindow">
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentGroup2" >
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentGroup4" inherits="PHGroupWindow">
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentGroup3" >
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Window>

            </Windows>
        </Window>

        <Button name="PHWindowClose" inherits="EA_Button_Default" backgroundtexture="shared_01" highlighttexture="shared_01" layer="popup">
            <Size>
                <AbsPoint x="37" y="37" />
            </Size>
            <Anchors>
                <Anchor point="topleft" relativePoint="topright" relativeTo="PHWindow">
                    <AbsPoint x="0" y="0" />
                </Anchor>
            </Anchors>

            <EventHandlers>
                <EventHandler event="OnLButtonUp" function="PartyHelper.OnClose" />
            </EventHandlers>

            <TexCoords>
                <Normal x="274" y="46" />
                <NormalHighlit x="274" y="83" />
                <Pressed x="274" y="120"  />
                <PressedHighlit x="274" y="120"  />
                <Disabled x="274" y="46" />
            </TexCoords>
        </Button>

        <Window name="PHWindow" layer="popup" movable="false" resizeable="false" handleinput="false" savesettings="true">
            <Size>
                <AbsPoint x="800" y="175" />
            </Size>

            <Anchors>
                <Anchor point="center" relativePoint="center">
                    <AbsPoint x="0" y="0" />
                </Anchor>
            </Anchors>

            <Windows>
                <Label name="$parentHeadText" inherits="EA_Label_DefaultText" textalign="left" maxchars="9999">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent" >
                            <AbsPoint x="10" y="0" />
                        </Anchor>
                        <Anchor point="topright" relativePoint="bottomright" relativeTo="$parent" >
                            <AbsPoint x="0" y="25" />
                        </Anchor>
                    </Anchors>

                </Label>
                <Window name="$parentWarband0" inherits="PHWarbandWindow">
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentHeadText">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentWarband1" inherits="PHWarbandWindow">
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentWarband0">
                            <AbsPoint x="0" y="25" />
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentWarband2" inherits="PHWarbandWindow">
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentWarband0">
                            <AbsPoint x="25" y="0" />
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentWarband3" inherits="PHWarbandWindow">
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentWarband2">
                            <AbsPoint x="0" y="25" />
                        </Anchor>
                    </Anchors>
                </Window>
            </Windows>
        </Window>
</Interface>