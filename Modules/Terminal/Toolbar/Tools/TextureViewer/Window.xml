<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
    <Windows>

        <Window name="TerminalTextureViewerIconEditor">
            <Size>
                <AbsPoint x="220" y="300"/>
            </Size>
            <Windows>
                <Window name="$parentBackground" inherits="EA_Window_DefaultBackgroundFrame"/>
                <Label name="$parentLabel" font="font_default_text" handleinput="false">
                    <Size>
                        <AbsPoint x="200" y="25"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="15" y="50"/>
                        </Anchor>
                    </Anchors>
                </Label>
                <Window name="$parentXOffset" inherits="warExtendedSmallEditBoxLabelDuo">
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="bottomleft">
                            <AbsPoint x="12" y="-12"/>
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentYOffset" inherits="warExtendedSmallEditBoxLabelDuo">
                    <Anchors>
                        <Anchor point="top" relativePoint="bottom" relativeTo="$parentXOffset">
                            <AbsPoint x="0" y="5"/>
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentXDimensions" inherits="warExtendedSmallEditBoxLabelDuo">
                    <Anchors>
                        <Anchor point="top" relativePoint="bottom" relativeTo="$parentYOffset">
                            <AbsPoint x="0" y="5"/>
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentYDimensions" inherits="warExtendedSmallEditBoxLabelDuo">
                    <Anchors>
                        <Anchor point="top" relativePoint="bottom" relativeTo="$parentXDimensions">
                            <AbsPoint x="0" y="5"/>
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentScale" inherits="warExtendedSmallEditBoxLabelDuo">
                    <Anchors>
                        <Anchor point="top" relativePoint="bottom" relativeTo="$parentYDimensions">
                            <AbsPoint x="0" y="5"/>
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentWidth" inherits="warExtendedSmallEditBoxLabelDuo">
                    <Anchors>
                        <Anchor point="top" relativePoint="bottom" relativeTo="$parentScale">
                            <AbsPoint x="0" y="5"/>
                        </Anchor>
                    </Anchors>
                </Window>
                <Window name="$parentHeigth" inherits="warExtendedSmallEditBoxLabelDuo">
                    <Anchors>
                        <Anchor point="top" relativePoint="bottom" relativeTo="$parentWidth">
                            <AbsPoint x="0" y="5"/>
                        </Anchor>
                    </Anchors>
                </Window>
            </Windows>
        </Window>

        <Button name="IconSelectionButton" inherits="DefaultIconButton" texturescale="0.859" font="font_default_text"
                textalign="bottomright" drawchildrenfirst="true">
        </Button>


        <Button name="TerminalTextureViewerIconSelection" inherits="IconSelectionButton">
            <EventHandlers>
                <!--	<EventHandler event="OnMouseDrag" function="EA_Window_Macro.IconMouseDrag" />
                    <EventHandler event="OnLButtonUp" function="EA_Window_Macro.IconLButtonUp" />
                    <EventHandler event="OnRButtonDown" function="EA_Window_Macro.IconRButtonDown" />-->
                <EventHandler event="OnMouseOver" function="FrameManager.OnMouseOver"/>
            </EventHandlers>
        </Button>


        <Button name="TerminalTextureViewerTabButton" inherits="warExtendedTopTab">
            <Size>
                <AbsPoint x="124" y="35"/>
            </Size>
            <TextOffset x="0" y="0"/>
        </Button>

        <Window name="TerminalTextureViewer" inherits="warExtendedDefaultWindow" savesettings="false">
            <Size>
                <AbsPoint x="630" y="600"/>
            </Size>
            <EventHandlers>
                <EventHandler event="OnInitialize" function="TerminalTextureViewer.OnInitialize"/>
            <!--    <EventHandler event="OnHidden" function="warExtendedTerminal.TextureViewerOnHidden"/>-->
            </EventHandlers>
            <Windows>
                <Window name="$parentTitleBar" inherits="warExtendedDefaultTitleBar"/>
                <Window name="$parentBackground" inherits="warExtendedDefaultBackground"/>
                <Button name="$parentResizeButton" inherits="warExtendedDefaultResizeButton">
                    <EventHandlers>
                        <EventHandler event="OnLButtonDown" function="warExtendedTerminal.TextureViewerOnResizeBegin"/>
                    </EventHandlers>
                </Button>

                <Window name="$parentTab">
                    <Size>
                        <AbsPoint x="500" y="35"/>
                    </Size>
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentTitleBar"/>
                        <Anchor point="bottomright" relativePoint="topright" relativeTo="$parentTitleBar"/>
                    </Anchors>
                    <Windows>
                        <Window name="$parentSeparatorLeft" inherits="EA_Window_TabSeparatorLeftSide" layer="background"
                                popable="false">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="bottomleft">
                                    <AbsPoint x="18" y="-6"/>
                                </Anchor>
                                <Anchor point="bottomleft" relativePoint="topright" relativeTo="$parentIcons">
                                    <AbsPoint x="0" y="-6"/>
                                </Anchor>
                            </Anchors>
                        </Window>

                        <Button name="$parentIcons" inherits="TerminalTextureViewerTabButton">
                            <Anchors>
                                <Anchor point="topleft" relativePoint="topleft">
                                    <AbsPoint x="40" y="0"/>
                                </Anchor>
                            </Anchors>
                        </Button>
                        <Button name="$parentTextures" inherits="TerminalTextureViewerTabButton">
                            <Anchors>
                                <Anchor point="right" relativePoint="left" relativeTo="$parentIcons">
                                    <AbsPoint x="0" y="0"/>
                                </Anchor>
                            </Anchors>
                        </Button>

                        <Window name="$parentSeparatorRight" inherits="EA_Window_TabSeparatorRightSide"
                                layer="background" popable="false">
                            <Anchors>
                                <Anchor point="bottomright" relativePoint="topleft" relativeTo="$parentTextures">
                                    <AbsPoint x="0" y="-6"/>
                                </Anchor>
                                <Anchor point="bottomright" relativePoint="bottomright">
                                    <AbsPoint x="-18" y="-6"/>
                                </Anchor>
                            </Anchors>
                        </Window>
                    </Windows>
                </Window>


                <Window name="$parentIcons" savesettings="false">
                    <Size>
                        <AbsPoint x="500" y="300"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="3" y="59"/>
                        </Anchor>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
                            <AbsPoint x="0" y="0"/>
                        </Anchor>
                    </Anchors>
                    <EventHandlers>
                        <EventHandler event="OnShown" function="FrameManager.OnShown"/>
                        <EventHandler event="OnHidden" function="FrameManager.OnHidden"/>
                    </EventHandlers>
                    <Windows>

                        <ScrollWindow name="$parentSelection" childscrollwindow="$parentScrollChild"
                                      scrollbar="$parentScrollbar" lineheight="40">
                            <Anchors>
                                <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                                    <AbsPoint x="7" y="6"/>
                                </Anchor>
                                <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentSeparator">
                                    <AbsPoint x="-7" y="-18"/>
                                </Anchor>
                            </Anchors>
                            <Windows>

                                <VerticalScrollbar name="$parentScrollbar" inherits="EA_ScrollBar_ChatVertical"
                                                   layer="popup">
                                    <Size>
                                        <AbsPoint x="20" y="238"/>
                                    </Size>
                                    <Anchors>
                                        <Anchor point="topright" relativePoint="topright" relativeTo="$parent">
                                            <AbsPoint x="-7" y="-3"/>
                                        </Anchor>
                                    </Anchors>
                                    <EventHandlers>
                                        <EventHandler event="OnScrollPosChanged"
                                                      function="FrameManager.OnScrollPosChanged"/>
                                    </EventHandlers>
                                </VerticalScrollbar>

                                <Window name="$parentScrollChild">
                                    <Size>
                                        <AbsPoint x="650" y="1000"/>
                                    </Size>
                                    <Anchors>
                                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                                            <AbsPoint x="1" y="-0"/>
                                        </Anchor>
                                    </Anchors>
                                </Window>
                            </Windows>
                        </ScrollWindow>

                        <Window name="$parentSeparator" inherits="EA_Window_DefaultSeparator" handleinput="false">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="bottomleft" parent="$parent">
                                    <AbsPoint x="-2" y="-270"/>
                                </Anchor>
                                <Anchor point="bottomright" relativePoint="bottomright" parent="$parent">
                                    <AbsPoint x="0" y="-270"/>
                                </Anchor>
                            </Anchors>
                        </Window>


                        <Window name="$parentDisplay" handleinput="true">
                            <Size>
                                <AbsPoint x="128" y="128"/>
                            </Size>
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="bottomleft" relativeTo="$parent">
                                    <AbsPoint x="60" y="80"/>
                                </Anchor>
                            </Anchors>
                            <Windows>
                                <FullResizeImage name="$parentFrame" inherits="EA_FullResizeImage_DefaultFrame">
                                    <Anchors>
                                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent"/>
                                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
                                            <AbsPoint x="7" y="6"/>
                                        </Anchor>
                                    </Anchors>
                                </FullResizeImage>
                                <FullResizeImage name="$parentBackground"
                                                 inherits="EA_FullResizeImage_TintableSolidBackground"
                                                 handleinput="true">
                                    <Anchors>
                                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                                            <AbsPoint x="10" y="6"/>
                                        </Anchor>
                                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
                                            <AbsPoint x="0" y="-2"/>
                                        </Anchor>
                                    </Anchors>
                                    <TintColor r="0" g="0" b="0"/>
                                </FullResizeImage>
                                <DynamicImage name="$parentIcon">
                                    <Anchors>
                                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                                            <AbsPoint x="11" y="7"/>
                                        </Anchor>
                                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
                                            <AbsPoint x="-2" y="-4"/>
                                        </Anchor>
                                    </Anchors>
                                    <EventHandlers>
                                        <EventHandler event="OnRButtonUp" function="IconViewer.OnIconRButtonUp"/>
                                    </EventHandlers>
                                </DynamicImage>
                            </Windows>
                        </Window>
                    </Windows>


                </Window>
            </Windows>
        </Window>


    </Windows>
</Interface>